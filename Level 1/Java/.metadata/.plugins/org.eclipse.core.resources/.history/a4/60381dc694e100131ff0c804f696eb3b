
public class Products {
	
	public String name;
	public double price;
	
	public void Product (String name, double price) {

		this.name = name;
		this.price = price;
	}
	
	public String getName() {
		
		return this.name;
	}
	
	public double getPrice() {
		
		return this.price;
	}
	
	public Products (String name, double price) {
		
		this.name = name;
		this.price = price;
	}
}
